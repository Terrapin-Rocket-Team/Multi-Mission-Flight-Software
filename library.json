        },
        {
            "name": "Joseph Hauerstein",
            "url": "https://github.com/jhauerst"
        },
        {
            "name": "Abhi Senthilkumar",
            "url": "https://github.com/abhi-senthilkumar"
        },
        {
            "name": "Avi Komarlingam",
            "url": "https://github.com/avikomarlingam"
        },
        {
            "name": "Drew Brandt",
            "url": "https://github.com/DrewBrandt"
        },
        {
            "name": "Terrapin Rocket Team",
            "url": "https://github.com/Terrapin-Rocket-Team"
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/Terrapin-Rocket-Team/Multi-Mission-Flight-Software",
    "dependencies": 
    [
        {
            "name": "Adafruit BMP280 Library",
            "version": "2.6.8"
        },
        {
            "name": "Adafruit BMP3XX Library",
            "version": "2.1.4"
        },
        {
            "name": "Adafruit BNO055",
            "version": "1.6.3"
        },
        {
            "name": "SparkFun u-blox GNSS v3",
            "version": "3.1.5"
        },
        {
            "name": "RTClib",
            "owner": "adafruit"
        },
        {
            "name": "RadioHead",
            "version": "https://github.com/Awbmilne/Patched-RadioHead.git"
        }
    ],
    "frameworks": "*",
    "platforms": "*",
    "build":
    {
        "libLDFMode": "deep+",
        "libCompatMode": "strict",
        "srcFilter": 
        [
            "+<*>"
        ],
        "flags": "-Wno-unknown-pragmas",
        "extraScript": "get_version.py"
    }
